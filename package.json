{
  "name": "magic-urls-server",
  "version": "1.0.0",
  "description": "Magic Urls Server",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && babel app -d build",
    "build-dev": "npm run clean && babel app --source-maps -d build",
    "clean": "rm -rf build && mkdir build",
    "start": "nodemon",
    "lint": "eslint app",
    "run-db": "docker run --name mongodb -v mongodata:/data/db -d -p 27017:27017 mongo"
  },
  "engines": {
    "node": ">= 6",
    "npm": ">= 3"
  },
  "keywords": [
    "express",
    "es6",
    "rest",
    "api",
    "mongo"
  ],
  "author": "Pavanello Emanuele <pavanello.emanuele@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git://github.com/epavanello/magic-urls-server.git"
  },
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.18.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.15.2",
    "cors": "^2.8.1",
    "dotenv": "^4.0.0",
    "express": "^4.14.0",
    "helmet": "^3.1.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.16.4",
    "method-override": "^2.3.6",
    "mongoose": "^4.6.5",
    "morgan": "^1.7.0",
    "multer": "^1.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.0.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "chai-http": "^4.3.0",
    "eslint": "^3.8.1",
    "eslint-config-google": "^0.7.1",
    "eslint-plugin-babel": "^4.0.0",
    "faker": "^3.1.0",
    "isparta": "^4.0.0",
    "mocha": "^6.1.4",
    "nodemon": "^1.11.0"
  }
}