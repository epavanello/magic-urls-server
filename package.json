{
  "name": "magic-urls-server",
  "version": "1.0.0",
  "description": "Magic Urls Server",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && babel --presets @babel/preset-env app -d build",
    "build-dev": "npm run clean && babel --presets @babel/preset-env app --source-maps -d build",
    "clean": "rm -rf build && mkdir build",
    "start": "nodemon",
    "lint": "eslint app",
    "run-db": "docker run --name mongodb -v mongodata:/data/db -d -p 27017:27017 mongo"
  },
  "engines": {
    "node": ">= 6",
    "npm": ">= 3"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/kylealwyn/node-rest-api-boilerplate.git"
  },
  "keywords": [
    "express",
    "es6",
    "rest",
    "api",
    "boilerplate",
    "mongo"
  ],
  "author": "Kyle Alwyn <kjalwyn@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kylealwyn/node-rest-api-boilerplate/issues"
  },
  "homepage": "https://github.com/kylealwyn/node-rest-api-boilerplate",
  "dependencies": {
    "babel-core": "^6.18.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.15.2",
    "cors": "^2.8.1",
    "dotenv": "^4.0.0",
    "express": "^4.14.0",
    "helmet": "^3.1.0",
    "jsonwebtoken": "^7.1.0",
    "lodash": "^4.16.4",
    "method-override": "^2.3.6",
    "mongoose": "^4.6.5",
    "morgan": "^1.7.0",
    "multer": "^1.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.0.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "eslint": "^3.8.1",
    "eslint-config-google": "^0.7.1",
    "eslint-plugin-babel": "^4.0.0",
    "faker": "^3.1.0",
    "isparta": "^4.0.0",
    "mocha": "^3.1.2",
    "nodemon": "^1.11.0"
  }
}